<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\MainBundle\Entity\Questionnaire" table="questionnaire">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false"/>
    <many-to-many field="customwave" target-entity="Customwave" inversed-by="questionnaire">
      <join-table name="questionnaire_customwave">
        <join-columns>
          <join-column name="questionnaire_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="customwave_id" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
