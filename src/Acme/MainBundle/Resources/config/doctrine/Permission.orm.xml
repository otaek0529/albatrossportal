<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\MainBundle\Entity\Permission" table="permission">
    <unique-constraints>
      <unique-constraint name="name_uniq" columns="name"/>
    </unique-constraints>
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="32" nullable="false"/>
    <field name="code" type="string" column="code" length="32" nullable="false"/>
    <field name="parameters" type="text" column="parameters" nullable="true"/>
  </entity>
</doctrine-mapping>
